<?xml version="1.0"?>
<Container version="2">
  <Name>Squid</Name>
  <Repository>manubocquet/unraid-squid</Repository>
  <Registry>https://hub.docker.com/r/manubocquet/unraid-squid/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Overview> Squid caching proxy
  </Overview>
  <TemplateURL/>
  <Icon>http://jebriggs.com/php/squid_logo.png</Icon>
  <ExtraParams/>
  <Description>
   Squid is a caching proxy for the Web supporting HTTP, HTTPS, FTP, and more. It reduces bandwidth and improves response times by caching and reusing frequently-requested web pages. Squid has extensive access controls and makes a great server accelerator.
  </Description>
  <Environment>
    <Variable>
      <Value>192.168.1.5:514</Value>
      <Name>SYSLOG_ADDR</Name>
      <Mode/>
    </Variable>
  </Environment>  
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3128</HostPort>
        <ContainerPort>3128</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>  
  <Data>
	 <Volume>
      <HostDir>/mnt/user/appdata/Squid3</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Config 
      Name="Host Port" 
      Target="3128" 
      Default="3128" 
      Mode="tcp" 
      Description="Container Port: 3128" 
      Type="Port" 
      Display="always" 
      Required="true" 
      Mask="false">3128
  </Config>
  <Config 
      Name="AppData Config Path" 
      Target="/config" 
      Default="/mnt/user/appdata/Squid3" 
      Mode="rw" 
      Description="Container Path: /config" 
      Type="Path" 
      Display="always" 
      Required="true" 
      Mask="false">/mnt/user/appdata/Squid3
  </Config>
  <Config 
       Type="Variable" 
       Name="Syslog address" 
       Target="SYSLOG_ADDR" 
       Default="192.168.1.5:514" 
       Description="Syslog destination (UPD) [default : 192.168.1.5:514]" 
       Mask="false" 
       Display="always" 
       Required="true">192.168.1.5:514
  </Config>
</Container>
