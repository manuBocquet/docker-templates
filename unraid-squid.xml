<?xml version="1.0"?>
<Container version="2">
  <Name>Squid</Name>
  <Repository>manubocquet/unraid-squid</Repository>
  <Registry>https://hub.docker.com/r/manubocquet/unraid-squid/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Overview> Fisrt docker 
  </Overview>
  <TemplateURL/>
  <Icon>http://jebriggs.com/php/squid_logo.png</Icon>
  <ExtraParams/>
  <Description>
   Description...v2
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3128</HostPort>
        <ContainerPort>3128</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
	 <Volume>
      <HostDir>/mnt/user/appdata/Squid3</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Config Name="Host Port 1" Target="3128" Default="3128" Mode="tcp" Description="Container Port: 3128" Type="Port" Display="always" Required="true" Mask="false">3128</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/appdata/Squid3" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/Squid3</Config>

</Container>
